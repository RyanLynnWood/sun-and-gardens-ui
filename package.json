    "less": "^1.7.5",
    "less-loader": "^0.7.8",
    "lite-server": "^2.2.2",
    "load-grunt-tasks": "^3.5.0",
    "node-sass": "^4.4.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^4.1.1",
    "script-loader": "0.6.1",
    "style-loader": "^0.13.1",
    "traceur-loader": "0.6.3",
    "ts-loader": "0.8.2",
